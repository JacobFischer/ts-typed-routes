{
  "name": "typesafe-routes",
  "version": "0.0.1",
  "description": "Routes built and re-usable with typesafe parameters",
  "main": "dist/index.js",
  "repository": "git@github.com:JacobFischer/Flexi-Stack-Boilerplate.git",
  "author": "Jacob Fischer <jacob.t.fischer@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/eslint": "4.16.6",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.12",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-import-order-alphabetical": "0.0.2",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-jsdoc": "10.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "shx": "0.3.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "clean": "shx rm -rf .cache/ dist/",
    "purge": "npm run clean && shx rm -rf node_modules/ && rm -rf package-lock.json",
    "lint": "eslint ./ --ext .js,.ts,tsx --ignore-pattern dist/",
    "test": "jest --env=node --config ./test/jest.config.js --colors --coverage test"
  }
}
